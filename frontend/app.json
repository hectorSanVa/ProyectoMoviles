{
  "expo": {
    "name": "Sistema Inventario",
    "slug": "sistema-inventario",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "sdkVersion": "54.0.0",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.inventario.app"
    },
    "android": {
      "package": "com.inventario.app",
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA"
      ]
    },
    "web": {},
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "de64d617-98ca-4617-a0e2-a33ee2483032"
      }
    }
  }
}
